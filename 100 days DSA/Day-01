//https://leetcode.com/problems/squares-of-a-sorted-array/description/
/*Using sort function:
Time complexity:O(nlogn)
Space complexity:O(1)*/

#include<bits/stdc++.h>
using namespace std;
int main()
{
    int array[11]={-5,-2,-2,1,0,2,3,5,5,7,};
    int ans[11];
     int start=0,end=10;
    for(int i=0;i<11;i++)
    {
     array[i]=array[i]*array[i];
    }
    sort(array,array+11);
 
 
 for(int i=0;i<11;i++)
 {
    cout<<"  "<<array[i];
 }
 
}

